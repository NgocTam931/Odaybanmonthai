	<script type="text/javascript">
		var isMobile = /iPhone|iPad|iPod|Android/i.test(navigator.userAgent);
		if (!isMobile) {
			var height = window.innerHeight;
			var number = 1288 / 1820;
			var width = height * number;
			$('#main').height(height);
			$('#main').width(width);
		} else {
			var width = window.innerWidth;
			var windowHeight = window.innerHeight;
			$('#wowslider-container1 .ws_images').css("max-height", "100%");
			$('#wowslider-container1 .ws_images').css("max-width", width + "px");

			var number = 1288 / 1820;
			var realHeight = width / number;

			var diff = windowHeight - realHeight;
			var margin = diff / 2;
			$('#wowslider-container1').css("margin-top", margin + "px");
		}
	</script>


